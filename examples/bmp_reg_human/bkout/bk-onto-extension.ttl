<http://www.ondex.org/bioknet/bk-onto-sample-extension/>
        a            owl:Ontology ;
        owl:imports  bk: .

bk:ExperimentalSample
        a                    owl:Class ;
        rdfs:label           "Experimental Sample"@en ;
        rdfs:subClassOf      bk:Concept ;
        terms:description    "Anything that can be seen as a sample related to organism(s) and taken to perform an experiment (e.g. blood sample, cell culture, cohort of patients)"@en ;
        terms:identifier     "ExperimentalSample" ;
        bk:isOndexPreferred  true .
